|#| 'stdio.nest' = io
|#| 'stdsutil.nest' = su
|#| 'stdsys.nest' = sys

$_args_ 3 != ? 1 @sys.exit
_args_.1 = path
_args_.2 = ext

'../../build/windows/projects/nest/x64/Debug/nest_' = dbg_path

path 'r' @io.open = file
file -1 @io.read \
    dbg_path 'std' @su.replace_substr \
    '.dll' ('.' ext ><) @su.replace_substr = content
file @io.close

path 'w' @io.open = file
file content @io.write
file @io.close
