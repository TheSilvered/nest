{
  "name": "nest-lang",
  "displayName": "Nest Lang",
  "description": "Syntax highlighting for the Nest programming language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "nest",
      "aliases": ["Nest", "nest"],
      "extensions": [".nest"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "nest",
      "scopeName": "source.nest",
      "path": "./syntaxes/nest.tmLanguage.json"
    }]
  }
}
