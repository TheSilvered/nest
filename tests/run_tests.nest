|#| 'stdfs.nest' = fs
|#| 'stdsequtil.nest' = seq

|#| 'object_tests/test_byte.nest'

#run_tests dir [
    ... Iter :: (dir @fs.list_dir_recursive) := test_path [
        test_path -5 -1 1 @seq.slice '.nest' != ? ..

        -- runs the file
        >>> (test_path '\n' ><)
        |#| test_path
    ]
]

'./object_tests' @run_tests
'./std_lib_tests' @run_tests
