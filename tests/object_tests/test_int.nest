|#| '../test_lib.nest' = test
|#| 'stdsys.nest' = sys

{ { 9223372036854775807, 9223372036854775807 },
  { -9223372036854775807, -9223372036854775807 },
  { 0, 0 },
  { 32, 32 },
  { 64, 64 },
  { (Int :: '10'), 10 },
  { (Int :: 10.5), 10 },
  { 9223372036854775807 1 +, -9223372036854775807 },
  { -9223372036854775807 1 -, 9223372036854775807 },
  { -:9223372036854775807, -9223372036854775807 } } = eq_tests

{ { 9223372036854775807, -9223372036854775807 },
  { 0, -1 },
  { 32, 31 },
  { 64, 63 },
  { (Int :: '10'), 11 },
  { (Int :: 10.5), 11 },
  { 9223372036854775807 1 -, 9223372036854775807 },
  { -9223372036854775807 1 +, 9223372036854775807 },
  { -:9223372036854775807, 9223372036854775807 } } = ne_tests

... Iter :: eq_tests ~= i [
    i.0 i.1 @test.assert_eq
]

... Iter :: new_tests ~= i [
    i.0 i.1 @test.assert_ne
]