|#| '../test_lib.nest' = test

{ { 256b, 0b },
  { -1b, 255b },
  { 10b, 10.0 },
  { 10b, 10 },
  { ?::10b, Byte },
  { Byte :: 'A', Byte :: '65b' },
  { 10b 10b +, 20b },
  { 20b 10b -, 10b },
  { 30b 30b *, 132b },
  { 25b 5b /, 5b },
  { 5b 2b ^, 25b },
  { Str :: 65b, 'A' } } = eq_tests

... Iter :: eq_tests := i [
    i.0 i.1 @test.assert_eq
]
