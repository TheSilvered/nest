|#| '../test_lib.nest' = test
|#| 'stdio.nest' = io

true  @test.assert_true
false @test.assert_false

0 0 == @test.assert_true
0 1 == @test.assert_false
0 1 != @test.assert_true
0 0 != @test.assert_false
0 1 < @test.assert_true
1 0 < @test.assert_false
1 0 > @test.assert_true
0 1 > @test.assert_false

@io.virtual_iof = f

Bool :: 0 @test.assert_false
Bool :: 1 @test.assert_true
Bool :: -1 @test.assert_true
Bool :: 0.0 @test.assert_false
Bool :: 1.0 @test.assert_true
Bool :: -1.0 @test.assert_true
Bool :: null @test.assert_false
Bool :: '' @test.assert_false
Bool :: 'a' @test.assert_true
Bool :: {,} @test.assert_false
Bool :: { 0 } @test.assert_true
Bool :: <{}> @test.assert_false
Bool :: <{ 0 }> @test.assert_true
Bool :: {} @test.assert_false
Bool :: { 'a': 1 } @test.assert_true
Bool :: ## [] @test.assert_true
Bool :: (0 -> 1) @test.assert_true
Bool :: 0b @test.assert_false
Bool :: 1b @test.assert_true
Bool :: f @test.assert_true
Bool :: Type @test.assert_true

f @io.close
Bool :: f @test.assert_false
