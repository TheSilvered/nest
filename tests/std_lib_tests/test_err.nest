|#| '../test_lib.nest' = test
|#| 'stderr.nest' = err
|#| 'stdfs.nest' = fs

#f [
    'Error' !! 'f'
]

f {,} @err.try = res
res.value null @test.assert_eq
res.error.name 'Error' @test.assert_eq
res.error.message 'f' @test.assert_eq
res.error.pos.start { 5, 4 } @test.assert_eq
res.error.pos.end { 5, 17 } @test.assert_eq
res.error.pos.file @fs.filename 'test_err.nest' @test.assert_eq
