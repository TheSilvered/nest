|#| '../test_lib.nest' = test
|#| 'stdutf8.nest' = utf8

'àèìòù' = s
'\xff' = i
s @utf8.is_valid @test.assert_true
i @utf8.is_valid @test.assert_false

s @utf8.get_len 5 @test.assert_eq
utf8.get_len { i } @test.assert_raises_error

Array :: (s @utf8.to_iter) { 'à', 'è', 'ì', 'ò', 'ù' } @test.assert_eq
##iter [=> Array :: iter] { i @utf8.to_iter } @test.assert_raises_error

s 0 @utf8.get_at 'à' @test.assert_eq
s 4 @utf8.get_at 'ù' @test.assert_eq
utf8.get_at { s, 5 } @test.assert_raises_error
utf8.get_at { i, 0 } @test.assert_raises_error
utf8.get_at { i, 5 } @test.assert_raises_error
