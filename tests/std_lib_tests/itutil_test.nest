|#| 'stditutil.nest' = itu

... 10 2 @itu.count ~= i [
    >>> (i ' ' ><)
    i 20 >= ? ;
]
>>> '\n'

0 = j
... 'Hi!' @itu.cycle ~= i [
    >>> (i ' ' ><)
    1 += j
    j 6 > ? ;
]
>>> '\n'

... 10 3 @itu.repeat ~= i [
    >>> (i ' ' ><)
]
>>> '\n'

... { { 1, 2 }, Array :: 'Hi' } @itu.chain ~= i [
    >>> (i ' ' ><)
]
>>> '\n'

... { 1, 2, 3 } 'Hi' @itu.zip ~= i [
    >>> (i ' ' ><)
]
>>> '\n'

... 'Hello' @itu.enumerate ~= i [
    >>> (i ' ' ><)
]
>>> '\n'

{ 'a': 1, 'b': 2, 'c': 3 } = m

... m @itu.keys ~= i [
    >>> (i ' ' ><)
]
>>> '\n'

... m @itu.values ~= i [
    >>> (i ' ' ><)
]
>>> '\n'

... m @itu.items ~= i [
    >>> (i ' ' ><)
]
>>> '\n'

... 'Hello' @itu.reversed ~= i [
    >>> (i ' ' ><)
]
>>> '\n'
